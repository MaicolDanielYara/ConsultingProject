@model IEnumerable<ConsultingProject.Core.Domain.Municipio>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_MenuAdmin.cshtml";
}

<h1>Contenido de Municipios</h1>

<p>
    <a asp-action="Create">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreMunicipio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CódigoDepartamentoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMunicipio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CódigoDepartamentoNavigation.CódigoDepartamento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CódigoMunicipio">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.CódigoMunicipio">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.CódigoMunicipio">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
