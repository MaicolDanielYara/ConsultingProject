</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" asp-area="" asp-controller="Home" asp-action="Index">
                <div class="sidebar-brand-icon rotate-n-15">
                    <img src="~/Img/logo_CP2.png" />
                </div>
                <div class="sidebar-brand-text mx-3">Consulting Project<sup></sup></div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <!-- Nav Item - Dashboard -->
            <li class="nav-item active">
                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                    <i class="fa fa-home" aria-hidden="true"></i>
                    <span>Inicio</span>
                </a>
            </li>
            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Interface
            </div>

            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" asp-area="Identity" asp-page="/Account/Manage/Index" data-toggle="collapse" data-target="#collapseTwo"
                   aria-expanded="true" aria-controls="collapseTwo">
                    <i class='fa fa-user-circle'></i>
                    <span>Mi Perfil</span>
                </a>
                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <h6 class="collapse-header">Acciones</h6>
                        <a class="collapse-item" asp-area="Identity" asp-page="/Account/Manage/Index">Editar Perfil</a>

                    </div>
                </div>
            </li>

            <!-- Nav Item - Utilities Collapse Menu -->
            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Administrar
            </div>

            <!-- Panel de Admin -->

            @if (User.IsInRole("Administrador"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseProyectos"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class="fas fa-chart-line"></i>
                        <span>Proyecto</span>
                    </a>
                    <div id="collapseProyectos" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Acciones</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Proyecto" asp-action="Index">Lista de Proyectos</a>
                            <a class="collapse-item" asp-area="" asp-controller="Proyecto" asp-action="Create">Crear Proyecto</a>
                            <a class="collapse-item" asp-area="" asp-controller="Actividad" asp-action="Create">Crear Actividad</a>
                            <a class="collapse-item" asp-area="" asp-controller="Etapa" asp-action="Create">Crear Etapa</a>
                        </div>
                    </div>
                </li>

            }

            <!-- Panel de Profesional -->

            @if (User.IsInRole("Profesional"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseProyectos"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class="fas fa-chart-line"></i>
                        <span>Proyecto</span>
                    </a>
                    <div id="collapseProyectos" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Acciones</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Proyecto" asp-action="Index">Lista de Proyectos</a>
                            <a class="collapse-item" asp-area="" asp-controller="Proyecto" asp-action="Create">Crear Proyecto</a>
                            <a class="collapse-item" asp-area="" asp-controller="Actividad" asp-action="Create">Crear Actividad</a>
                            <a class="collapse-item" asp-area="" asp-controller="Etapa" asp-action="Create">Crear Etapa</a>
                        </div>
                    </div>
                </li>
            }
            @if (User.IsInRole("Consultor"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseProyectos"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class="fas fa-chart-line"></i>
                        <span>Proyecto</span>
                    </a>
                    <div id="collapseProyectos" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Acciones</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Proyecto" asp-action="Index">Lista de Proyectos</a>
                            <a class="collapse-item" asp-area="" asp-controller="Actividad" asp-action="Index">Lista de Actividades</a>
                            <a class="collapse-item" asp-area="" asp-controller="Etapa" asp-action="Index">Lista de Etapas</a>
                        </div>
                    </div>
                </li>
            }
            <!-- Proyectos -->
            <!-- Empresas -->
            @if (User.IsInRole("Administrador"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseEmpresas"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class='fa fa-industry'></i>
                        <span>Empresas</span>
                    </a>
                    <div id="collapseEmpresas" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Acciones</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Empresa" asp-action="Index">Ver empresas</a>
                            <a class="collapse-item" asp-area="" asp-controller="ContactoEmpresa" asp-action="index">Contacto de Emrpesas</a>
                        </div>
                    </div>
                </li>
            }

            @if (User.IsInRole("Profesional"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseEmpresas"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class='fa fa-industry'></i>
                        <span>Empresas</span>
                    </a>
                    <div id="collapseEmpresas" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Acciones</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Empresa" asp-action="Index">Ver empresas</a>
                            <a class="collapse-item" asp-area="" asp-controller="ContactoEmpresa" asp-action="index">Contacto de Emrpesas</a>
                        </div>
                    </div>
                </li>
            }
            @if (User.IsInRole("Consultor"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseEmpresas"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class='fa fa-industry'></i>
                        <span>Empresas</span>
                    </a>
                    <div id="collapseEmpresas" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Acciones</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Empresa" asp-action="Index">Ver empresas</a>
                            <a class="collapse-item" asp-area="" asp-controller="ContactoEmpresa" asp-action="index">Contacto de Emrpesas</a>
                        </div>
                    </div>
                </li>
            }


            @if (User.IsInRole("Administrador"))
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapsePages"
                       aria-expanded="true" aria-controls="collapsePages">
                        <i class='fa fa-table'></i>
                        <span>Tablas</span>
                    </a>
                    <div id="collapsePages" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Listado</h6>
                            <a class="collapse-item" asp-area="" asp-controller="Actividad" asp-action="Index">Actividades</a>
                            <a class="collapse-item" asp-area="" asp-controller="Cargo" asp-action="Index">Cargos</a>
                            <a class="collapse-item" asp-area="" asp-controller="ContactoEmpresa" asp-action="Index">Contacto de Empresas</a>
                            <a class="collapse-item" asp-area="" asp-controller="Departamento" asp-action="Index">Departamentos</a>
                            <a class="collapse-item" asp-area="" asp-controller="EjeSeleccionado" asp-action="Index">Ejes Seleccionados</a>
                            <a class="collapse-item" asp-area="" asp-controller="Empresa" asp-action="Index">Empresa</a>
                            <a class="collapse-item" asp-area="" asp-controller="Estudio" asp-action="Index">Estudios</a>
                            <a class="collapse-item" asp-area="" asp-controller="Etapa" asp-action="Index">Etapas</a>
                            <a class="collapse-item" asp-area="" asp-controller="Herramienta" asp-action="Index">Herramientas</a>
                            <a class="collapse-item" asp-area="" asp-controller="Municipio" asp-action="Index">Municipios</a>
                            <a class="collapse-item" asp-area="" asp-controller="Profesion" asp-action="Index">Profesiones</a>
                            <a class="collapse-item" asp-area="" asp-controller="Profesional" asp-action="Index">Profesionales</a>
                            <a class="collapse-item" asp-area="" asp-controller="Proyecto" asp-action="Index">Proyectos</a>
                            <a class="collapse-item" asp-area="" asp-controller="RepresentanteLegal" asp-action="Index">Representante Legal</a>
                            <a class="collapse-item" asp-area="" asp-controller="Rol" asp-action="Index">Roles</a>
                            <a class="collapse-item" asp-area="" asp-controller="Sector" asp-action="Index">Sectores</a>
                            <a class="collapse-item" asp-area="" asp-controller="TamañoEmpresa" asp-action="Index">Tamaño de Empresas</a>
                            <a class="collapse-item" asp-area="" asp-controller="TipoDocumento" asp-action="Index">Tipo de Documento</a>


                        </div>
                    </div>
                </li>
            }

            <!-- Nav Item - Pages Collapse Menu -->


            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseUtilities"
                   aria-expanded="true" aria-controls="collapseUtilities">
                    <i class="fa fa-book" aria-hidden="true"></i>
                    <span>Consultoría</span>
                </a>
                <div id="collapseUtilities" class="collapse" aria-labelledby="headingUtilities"
                     data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <h6 class="collapse-header">Acciones</h6>
                        <a class="collapse-item" asp-area="" asp-controller="Home" asp-action="CitaConsultoria">Programar Cita</a>
                        <a class="collapse-item" asp-area="" asp-controller="Home" asp-action="Estadísticas">Avances de Consultoría</a>
                    </div>
                </div>
            </li>

            <!-- Nav Item - Charts -->
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Estadísticas">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Estadísticas</span>
                </a>
            </li>
            @if (User.IsInRole("Administrador"))
            {
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Usuario" asp-action="Index">
                        <i class="fa fa-users" aria-hidden="true"></i>
                        <span>Administrar Usuarios</span>
                    </a>
                </li>
            }
            <!-- Nav Item - Tables -->
            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>

            <!-- Sidebar Message -->
            <div class="sidebar-card">
                <img class="sidebar-card-illustration mb-2" src="~/Img/logo_CP.png" alt="">
                <p class="text-center mb-2"><strong>ConsultingProject</strong></p>
            </div>

        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <!-- Contenido de navegacion arriba-->
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">

                    @RenderBody()

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>ConsultingProjectWeb</span>
                        <span>2021</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="login.html">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>
