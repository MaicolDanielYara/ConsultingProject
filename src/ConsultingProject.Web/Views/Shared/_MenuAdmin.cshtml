    @RenderSection("EstudioIndex", false)
    @RenderSection("EtapaIndex", false)
    @RenderSection("HerramientaIndex", false)
    @RenderSection("MunicipioIndex", false)
    @RenderSection("ProfesionIndex", false)
    @RenderSection("ProfesionalIndex", false)
    @RenderSection("ProyectoIndex", false)
