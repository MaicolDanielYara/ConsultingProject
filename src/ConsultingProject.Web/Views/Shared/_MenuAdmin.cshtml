

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>ConsultingProject</title>

    <!-- Custom fonts for this template-->
    <link href="~/lib/fontawesome-free/css/all.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet" />

    @RenderSection("StyleAboutUs", false)
    @RenderSection("StyleLogin", false)
    @RenderSection("Index", false)
    @RenderSection("ActividadIndex", false)
    @RenderSection("CargosIndex", false)
    @RenderSection("ContactoEmpresaIndex", false)
    @RenderSection("DepartamentoIndex", false)
    @RenderSection("EjeSeleccionadoIndex", false)
